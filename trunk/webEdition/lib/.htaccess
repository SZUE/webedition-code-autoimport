<FilesMatch "\.php$">
	<ifModule mod_authz_core>
		Require all denied
	</ifModule>
	<ifModule !mod_authz_core>
		Order Allow,Deny
		Deny from all
	</ifModule>
</FilesMatch>
<Files "TreeSuggest.php">
	<ifModule mod_authz_core>
		Require all granted
	</ifModule>
	<ifModule !mod_authz_core>
		Order Allow,Deny
		Allow from all
	</ifModule>
</Files>
