<FilesMatch "\.inc.php$">
	<ifModule mod_authz_core>
		Require all denied
	</ifModule>
	<ifModule !mod_authz_core>
		Order Allow,Deny
		Deny from all
	</ifModule>
</FilesMatch>
<FilesMatch "\.class.php$">
	<ifModule mod_authz_core>
		Require all denied
	</ifModule>
	<ifModule !mod_authz_core>
		Order Allow,Deny
		Deny from all
	</ifModule>
</FilesMatch>
<FilesMatch "\.conf.php$">
	<ifModule mod_authz_core>
		Require all denied
	</ifModule>
	<ifModule !mod_authz_core>
		Order Allow,Deny
		Deny from all
	</ifModule>
</FilesMatch>
